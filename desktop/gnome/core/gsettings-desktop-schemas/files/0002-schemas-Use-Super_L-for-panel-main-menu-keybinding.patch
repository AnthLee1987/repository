Subject: [PATCH 2/2] schemas: Use Super_L for panel-main-menu keybinding

---
 schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in.in | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in.in b/schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in.in
index a502fef..5ad6427 100644
--- a/schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in.in
+++ b/schemas/org.gnome.desktop.wm.keybindings.gschema.xml.in.in
@@ -128,7 +128,7 @@
       <_summary>Hide all normal windows</_summary>
     </key>
     <key name="panel-main-menu" type="as">
-      <default><![CDATA[['<Super>s','<Alt>F1']]]></default>
+      <default><![CDATA[['Super_L']]]></default>
       <_summary>Show the activities overview</_summary>
     </key>
     <key name="panel-run-dialog" type="as">
